pipeline{
    
    agent any
        
    stages{
         stage("Appium-start"){
            steps{
                sh "appium"
            }
        }
        stage('run_py'){
            steps{
                sh """
                   cd Tests
                   pytest -v -s test_login.py --platformname Android --udid 988a90363239513753 --platformversion 8.0.0
                  
                """  
            }
        }
       
        
    }
        
}
